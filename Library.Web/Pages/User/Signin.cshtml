@page "/signin"
@model Library.Web.Pages.User.SigninPageModel
@{
}

<form method="post" enctype="text/plain">
    <div class="form-group mt-5">
        <h5 class="mt-3">Username</h5>
        <input asp-for="SigninModel.Username" class="form-control" />
    </div>

    <div class="form-group mt-5">
        <h5 class="mt-3">Phone number</h5>
        <input asp-for="SigninModel.PhoneNumber" class="form-control" />
    </div>

    <div class="form-group mt-5">
        <h5 class="mt-3">Password</h5>
        <input asp-for="SigninModel.Password" class="form-control" />
    </div>

    <button type="submit">submit</button>
</form>